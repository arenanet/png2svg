{
  "name": "png2svg",
  "version": "1.0.0",
  "dependencies": {
    "asynquence": "^0.1.0",
    "color-transform": "^1.0.1",
    "concat-stream": "^1.5.0",
    "jpg-stream": "^1.1.1",
    "lodash.template": "^3.6.2",
    "minimist": "^1.1.3",
    "png-stream": "^1.0.4"
  },
  "description": "This is a simple tool invoked from the command line which converts a png into an svg that uses jpeg compression.  This can significantly reduce image sizes for use on the web.",
  "devDependencies": {},
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arenanet/png2svg.git"
  },
  "keywords": [
    "png",
    "svg",
    "jpg",
    "optimization"
  ],
  "author": "ArenaNet, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arenanet/png2svg/issues"
  },
  "homepage": "https://github.com/arenanet/png2svg#readme",
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true
    },
    "rules": {
      "curly": 2,
      "dot-notation": 2,
      "eol-last": 0,
      "key-spacing": [
        2,
        {
          "beforeColon": true,
          "afterColon": true,
          "align": "colon"
        }
      ],
      "no-caller": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-eval": 2,
      "no-ex-assign": 2,
      "no-extend-native": 2,
      "no-extra-parens": 2,
      "no-extra-semi": 2,
      "no-floating-decimal": 2,
      "no-func-assign": 2,
      "no-implied-eval": 2,
      "no-inner-declarations": 2,
      "no-mixed-requires": 0,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-negated-in-lhs": 2,
      "no-new": 0,
      "no-obj-calls": 2,
      "no-sparse-arrays": 2,
      "no-trailing-spaces": 0,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2
    }
  }
}
